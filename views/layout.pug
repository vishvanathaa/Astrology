        include partials/header
        include partials/flash
        block layout-content
        div.modal.fade(id='signmodalAries' role='dialog' area-hidden='true')
              div.modal-dialog.modal-dialog-centered(role='document')
                div.modal-content
                  div.modal-body
                    div.row(style="padding-left:15px;")
                      div.col-md-4.va-spinner
                        i.fa.fa-spinner.fa-spin.va-location &nbsp; WAIT
                      div.col-md-8 
                        h5
                        i.fa.fa-star &nbsp;Astrology is a language. If you understand this language, the sky speaks to you. 
                        span
                          i.fa.fa-cog
                          i.fa.fa-cog
